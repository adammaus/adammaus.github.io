---
layout: null
---
[
  {% for page in site.posts %}
    {
      "title": "{{ page.title | escape }}",
      "url": "{{ page.url | relative_url }}",
      "tags": "{{ post.tags | join: ', ' }}",
      "excerpt": "{{ page.excerpt | strip_html | strip_newlines | jsonify | escape | replace: '\&quot;', '' | replace: '&quot;', '' | replace: '\t', ' ' | replace: '\U+00a0', ' ' }}",
      "content": "{{ page.content | strip_html | strip_newlines | jsonify | escape | replace: '\&quot;', '' | replace: '&quot;', '' | replace: '\t', ' ' | replace: '\U+00a0', ' ' }}"
    }{% unless forloop.last %},{% endunless %}
  {%- endfor %}
]